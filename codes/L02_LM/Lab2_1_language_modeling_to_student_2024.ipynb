        "def perplexity(bigram_data,model):\n",
        "    total_ln_prob = 0.0\n",
        "    for bi in bigram_data[0]:\n",
        "        ln_prob = getLnValue(model[bi])\n",
        "        total_ln_prob += ln_prob\n",
        "    N = len(bigram_data[0])\n",
        "    perplexity = math.exp(-total_ln_prob/N)\n",
        "    return perplexity"
      ]
